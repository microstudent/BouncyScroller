<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VerticalIndexBar">
        <attr name="bg" format="reference" />
    </declare-styleable>
    <declare-styleable name="AbsRecyclerViewScroller">
        <attr name="behavior" format="enum">
            <enum name="simple" value="0" />
            <enum name="show_index_in_need" value="1" />
            <enum name="always_show_index"  value="2"/>
        </attr>
        <attr name="theme_color" format="color" />
        <attr name="alpha2" format="float" />
    </declare-styleable>
</resources>
